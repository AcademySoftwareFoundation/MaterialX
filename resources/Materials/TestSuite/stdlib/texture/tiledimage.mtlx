<?xml version="1.0"?>
<materialx version="1.38">
  <!--
  Basic tiled image unit test with one image node for each variation in input type.
  A scale and offset of 2,2 and 0.5,0.5 is applied to the texture coordinates
  used to lookup each image
  -->
  <output name="tiled_image4_output" type="color4" nodename="tiled_image4" />
  <tiledimage name="tiled_image4" type="color4">
    <input name="file" type="filename" value="resources/Images/grid.png" />
    <input name="uvtiling" type="vector2" value="2.0, 2.0" />
    <input name="uvoffset" type="vector2" value="0.5, 0.5" />
    <input name="filtertype" type="string" value="cubic" />
    <input name="frameendaction" type="string" value="constant" />
  </tiledimage>
  <output name="tiled_image3_output" type="color3" nodename="tiled_image3" />
  <tiledimage name="tiled_image3" type="color3">
    <input name="file" type="filename" value="resources/Images/grid.png" />
    <input name="uvtiling" type="vector2" value="2.0, 2.0" />
    <input name="uvoffset" type="vector2" value="0.5, 0.5" />
    <input name="filtertype" type="string" value="closest" />
    <input name="frameendaction" type="string" value="clamp" />
  </tiledimage>
  <output name="tiled_image2_output" type="vector2" nodename="tiled_image2" />
  <tiledimage name="tiled_image2" type="vector2">
    <input name="file" type="filename" value="resources/Images/grid.png" />
    <input name="uvtiling" type="vector2" value="2.0, 2.0" />
    <input name="uvoffset" type="vector2" value="0.5, 0.5" />
    <input name="filtertype" type="string" value="linear" />
    <input name="frameendaction" type="string" value="periodic" />
  </tiledimage>
  <output name="tiled_image1_output" type="float" nodename="tiled_image1" />
  <tiledimage name="tiled_image1" type="float">
    <input name="file" type="filename" value="resources/Images/grid.png" />
