void mx_rotate2d_vector2(vector in, float amount, vector center, out vector result)
{
    float rotationRadians = radians(amount);
    float sa = sin(rotationRadians);
    float ca = cos(rotationRadians);
    vector v = in + center;
    result = vector(ca*v[0] + sa*v[1], -sa*v[0] + ca*v[1], 0.0) - center;
}
