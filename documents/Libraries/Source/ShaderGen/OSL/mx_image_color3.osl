void mx_image_color3(string file, string layer, color default_value, vector texcoord, string uaddressmode, string vaddressmode, string framerange, int frameoffset, string frameendaction, output color out)
{
    vdirection(texcoord, texcoord);
    color missingColor = default_value;
    out = texture(file, texcoord[0], texcoord[1], "subimage", layer, "missingcolor", missingColor, "wrap", uaddressmode);
}
