#include "mx_funcs.h"


shader mx_subtract_float
    [[ string help = "Float"" subtract in2 from the incoming data" ]]
  (
    float in1 = 0,
    float in2 = 0,
    output float out = 0
  )
{
    out = mx_sub (in1, in2);
}
