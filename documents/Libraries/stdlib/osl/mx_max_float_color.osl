#include "mx_funcs.h"

shader mx_max_float_color
    [[ string help = "Color"" Select the maximum among incoming values." ]]
  (
    color in1 = 0,
    float in2 = 0.0,
    output color out = 0
  )
{
    out = max(in1, in2); 
}
