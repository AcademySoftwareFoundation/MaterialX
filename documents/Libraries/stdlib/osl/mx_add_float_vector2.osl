#include "mx_funcs.h"

shader mx_add_float_vector2
    [[ string help = "Vector2"" add in2 to the incoming data" ]]
  (
    vector2 in1 = {0,0},
    float in2 = 0.0,
    output vector2 out = {0,0}
  )
{
    out = mx_add (in1, in2);
}
