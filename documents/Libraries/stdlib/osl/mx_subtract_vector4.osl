#include "mx_funcs.h"


shader mx_subtract_vector4
    [[ string help = "Vector4"" subtract in2 from the incoming data" ]]
  (
    vector4 in1 = {0,0,0,0},
    vector4 in2 = {0,0,0,0},
    output vector4 out = {0,0,0,0}
  )
{
    out = mx_sub (in1, in2);
}
