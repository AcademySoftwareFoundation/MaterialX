#include "mx_funcs.h"

shader mx_ceil_vector
    [[ string help = "Vector"" Find the smallest integer less than or equal to the parameter." ]]
  (
    vector in = 0,
    output vector out = 0
  )
{
    out = ceil(in); 
}
