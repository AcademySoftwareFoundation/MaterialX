#include "mx_funcs.h"

shader mx_divide_vector
    [[ string help = "Vector"" divide an incoming float/color/vector by in2" ]]
  (
    vector in1 = 0,
    vector in2 = 1,
    output vector out = 0
  )
{
    out = in1 / in2;
}
