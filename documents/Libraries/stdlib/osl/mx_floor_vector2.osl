#include "mx_funcs.h"

shader mx_floor_vector2
    [[ string help = "Vector2"" Find the nearest integer less than or equal to the parameter." ]]
  (
    vector2 in = {0,0},
    output vector2 out = {0,0}
  )
{
    out = floor(in); 
}
