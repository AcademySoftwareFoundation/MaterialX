#include "mx_funcs.h"

shader mx_sqrt_color4
    [[ string help = "Color4"" sqrt(in)" ]]
  (
    color4 in = {color(0,0,0), 0},
    output color4 out = {color(0,0,0), 0}
  )
{
    out = sqrt(in);
}
