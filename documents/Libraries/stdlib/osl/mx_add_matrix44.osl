#include "mx_funcs.h"


shader mx_add_matrix44
    [[ string help = "Matrix44"" add in2 to the incoming data" ]]
  (
    matrix in1 = matrix(0),
    matrix in2 = matrix(0),
    output matrix out = matrix(0)
  )
{
    out = mx_add (in1, in2);
}
