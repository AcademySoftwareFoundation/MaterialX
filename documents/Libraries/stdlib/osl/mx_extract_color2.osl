#include "mx_funcs.h"


shader mx_extract_color2
    [[ string help = "Generate a float from an aggregate (vector, color, etc.)" ]]
  (
    color2 in = {0,0},
    int index = 0,
    output float out = 0
  )
{
    out = extract (in, index);
}
