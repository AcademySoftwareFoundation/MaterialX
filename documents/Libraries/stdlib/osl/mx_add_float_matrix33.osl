#include "mx_funcs.h"

shader mx_add_float_matrix33
    [[ string help = "Matrix33"" add in2 to the incoming data" ]]
  (
    matrix in1 = matrix(0),
    float in2 = 0.0,
    output matrix out = matrix(0)
  )
{
    out = mx_add (in1, in2);
}
