#include "mx_funcs.h"


shader mx_subtract_color4
    [[ string help = "Color4"" subtract in2 from the incoming data" ]]
  (
    color4 in1 = {color(0,0,0), 0},
    color4 in2 = {color(0,0,0), 0},
    output color4 out = {color(0,0,0), 0}
  )
{
    out = mx_sub (in1, in2);
}
