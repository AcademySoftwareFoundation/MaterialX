#include "mx_funcs.h"

shader mx_atan2_float_color
    [[ string help = "Color"" atan2(in1, in2)" ]]
  (
    color in1 = 0,
    float in2 = 0.0,

    output color out = 0
  )
{
    out = atan2(in1, in2);
}
