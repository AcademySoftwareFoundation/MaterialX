#include "mx_funcs.h"


shader mx_add_color
    [[ string help = "Color"" add in2 to the incoming data" ]]
  (
    color in1 = 0,
    color in2 = 0,
    output color out = 0
  )
{
    out = mx_add (in1, in2);
}
