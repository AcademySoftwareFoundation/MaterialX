#include "mx_funcs.h"

shader mx_atan2_color2
    [[ string help = "Color2"" atan2(in1, in2)" ]]
  (
    color2 in1 = {0,0},
    color2 in2 = {0,0},

    output color2 out = {0,0}
  )
{
    out = atan2(in1, in2);
}
