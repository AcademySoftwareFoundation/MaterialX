#include "mx_funcs.h"

shader mx_subtract_float_matrix44
    [[ string help = "Matrix44"" subtract in2 from the incoming data" ]]
  (
    matrix in1 = matrix(0),
    float in2 = 0.0,
    output matrix out = matrix(0)
  )
{
    out = mx_sub(in1, in2);
}
