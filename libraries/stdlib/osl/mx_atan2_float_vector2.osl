#include "mx_funcs.h"

shader mx_atan2_float_vector2
    [[ string help = "Vector2"" atan2(in1, in2)" ]]
  (
    vector2 in1 = {0,0},
    float in2 = 0.0,

    output vector2 out = {0,0}
  )
{
    out = atan2(in1, in2);
}
