#include "mx_funcs.h"

shader mx_atan2_float
    [[ string help = "Float"" atan2(in1, in2)" ]]
  (
    float in1 = 0,
    float in2 = 0,

    output float out = 0
  )
{
    out = atan2(in1, in2);
}
