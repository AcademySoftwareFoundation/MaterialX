#include "mx_funcs.h"

shader mx_subtract_float_vector
    [[ string help = "Vector"" subtract in2 from the incoming data" ]]
  (
    vector in1 = 0,
    float in2 = 0.0,
    output vector out = 0
  )
{
    out = mx_sub(in1, in2);
}
