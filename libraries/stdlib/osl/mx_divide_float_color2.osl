#include "mx_funcs.h"

shader mx_divide_float_color2
    [[ string help = "Color2"" Divide an incoming float/color/vector by the in2" ]]
  (
    color2 in1 = {0,0},
    float in2 = 1.0,
    output color2 out = {0,0}
  )
{
    out = in1 / in2; 
}
