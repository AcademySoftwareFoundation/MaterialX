#include "mx_funcs.h"

shader mx_max_vector2
    [[ string help = "Vector2""  Select the maximum among incoming values." ]]
  (
    vector2 in1 = {0,0},
    vector2 in2 = {0,0},
    output vector2 out = {0,0}
  )
{
    out = max(in1, in2); 
}
