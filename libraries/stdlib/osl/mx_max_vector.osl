#include "mx_funcs.h"

shader mx_max_vector
    [[ string help = "Vector""  Select the maximum among incoming values." ]]
  (
    vector in1 = 0,
    vector in2 = 0,
    output vector out = 0
  )
{
    out = max(in1, in2); 
}
