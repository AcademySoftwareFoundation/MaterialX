#include "mx_funcs.h"

shader mx_clamp_color2 
    [[ string help = "Color2"" Clamp incoming value to a specified range of values." ]]
  (
    color2 in = {0,0},
    color2 low = {0,0},
    color2 high = {1,1},
    output color2 out = {0,0}
  )
{
    out = clamp(in, low, high);
}
