#include "mx_funcs.h"

shader mx_max_float_color2
    [[ string help = "Color2"" Select the maximum among incoming values." ]]
  (
    color2 in1 = {0,0},
    float in2 = 0.0,
    output color2 out = {0,0}
  )
{
    out = max(in1, in2); 
}
