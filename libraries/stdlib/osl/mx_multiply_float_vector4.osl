#include "mx_funcs.h"

shader mx_multiply_float_vector4
    [[ string help = "Vector4"" multiply in1 by in2" ]]
  (
    vector4 in1 = {0,0,0,0},
    float in2 = 1.0,
    output vector4 out = {0,0,0,0}
  )
{
    out = in1 * in2; 
}
