#include "mx_funcs.h"

shader mx_multiply_color2
    [[ string help = "Color2"" multiply in1 by in2" ]]
  (
    color2 in1 = {0,0},
    color2 in2 = {1,1},
    output color2 out = {0,0}
  )
{
    out = in1 * in2;
}
